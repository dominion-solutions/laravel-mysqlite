{
    "name": "spam-n-eggs/laravel-mysqlite",
    "description": "MySQLite connector for laravel that Leverages vectorface/mysqlite to add MySQL functionality to SQLite",
    "type": "library",
    "require": {
        "vectorface/mysqlite": "^0.1.4",
        "illuminate/database": "^5.7",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "codedungeon/phpunit-result-printer": "dev-master",
        "phpunit/php-code-coverage": "^6.1@dev",
        "php-coveralls/php-coveralls": "2.1.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark J. Horninger",
            "email": "thisisafakeemailaddr@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mhorninger\\": "src/Mhorninger/"
        }
    },
    "minimum-stability": "dev"
}
